{
    "name": "pixel",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/boringcrypto/pixel.git",
    "author": "BoringCrypto <b@rtje.net>",
    "license": "MIT",
    "scripts": {
        "test": "hardhat test",
        "format": "prettier --write contracts/**/*.sol *.js *.ts *.json test/**/*.js src/**/*.ts src/**/*.js src/**/*.json **/*.vue",
        "pretty-quick": "pretty-quick",
        "coverage": "hardhat coverage",
        "flat": "hardhat flat .\\contracts\\Pixel.sol --output .\\contracts\\PixelFlat.sol",
        "ts": "yarn run typechain --target ethers-v5 \"artifacts/contracts/**/?(PixelV2|PixelMigrator|MiniChefV2).json\"",
        "build": "vue-tsc --noEmit && vite build",
        "dev": "hardhat compile & yarn ts & concurrently \"hardhat node\" \"vite\""
    },
    "dependencies": {
        "bootstrap": "^5.0.0-beta3",
        "ethers": "^5.1.4",
        "vue": "^3.0.5",
        "vue-router": "4",
        "vue-tsc": "^0.0.24"
    },
    "devDependencies": {
        "98.css": "^0.1.17",
        "@boringcrypto/boring-solidity": "boringcrypto/BoringSolidity#77f3752c4bcf47cb7f8e8140ac8cd626b1151ad3",
        "@boringcrypto/hardhat-framework": "boringcrypto/hardhat-framework#8bc714f70dc1075cfcaa0fa0c6d4e4b9624cf14c",
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@types/express": "^4.17.13",
        "@types/node": "^16.4.13",
        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.5",
        "clippyjs": "^0.0.3",
        "concurrently": "^6.2.0",
        "decimal.js-light": "^2.5.1",
        "express": "^4.17.1",
        "sass": "^1.32.12",
        "ts-node": "^10.1.0",
        "typescript": "^4.1.3",
        "vite": "^2.2.3"
    }
}
